    "def email_slicer():\n",
    "    email = input(\"Enter your Email id: \").strip()\n",
    "    username = email[:email.index('@')]\n",
    "    domain = email[email.index('@')+1:]\n",
    "    print(\"\\nYour username is\",username,\"& your domain is\",domain)"
   ]
